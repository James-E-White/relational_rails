<%= form_with url: '/stores', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :square_footage %>
  <%= form.number_field :square_footage %>
  <%= form.label :online_sales %>
  <%= form.text_field :online_sales %>

  <%= form.submit "Create Store" %>
<% end %>